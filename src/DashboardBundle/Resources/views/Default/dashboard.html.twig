{% extends '::base.html.twig' %}
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('plugins/morris/morris.css') }}">
        <link rel="stylesheet" href="{{ asset('plugins/fullcalendar/fullcalendar.css') }}">
        <link rel="stylesheet" href="{{ asset('plugins/jvectormap/jquery-jvectormap-1.2.2.css') }}">
    {% endblock %}

    {% block title %} Accueil - Dashboard {% endblock %}
    {% block title2 %} Accueil {% endblock %}
    {% block title3 %} Dashboard {% endblock %}
    {% block breadcrumb %} Dashboard {% endblock %}
{% block body %}
    <div class="col-sm-8">
        {% embed 'DashboardBundle:Default:calendar.html.twig'  %}
        {% endembed %}
    </div>
    <div class="col-sm-4">
        {% embed 'DashboardBundle:Default:latest_members.html.twig'  %}
        {% endembed %}
        {% embed 'DashboardBundle:Default:todo_list.html.twig'  %}
        {% endembed %}
    </div>
    <div class="col-sm-12">
        {% embed 'DashboardBundle:Default:graph_recap.html.twig'  %}
        {% endembed %}
    </div>
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('js/raphael-min.js') }}"></script>
    <script src="{{ asset('plugins/morris/morris.min.js') }}"></script>
    <script src="{{ asset('plugins/sparkline/jquery.sparkline.min.js') }}"></script>
    <script src="{{ asset('plugins/jvectormap/jquery-jvectormap-1.2.2.min.js') }}"></script>
    <script src="{{ asset('plugins/jvectormap/jquery-jvectormap-world-mill-en.js') }}"></script>
    <script src="{{ asset('plugins/knob/jquery.knob.js') }}"></script>
    <script src="{{ asset('js/moment.min.js') }}"></script>
    <script src="{{ asset('plugins/slimScroll/jquery.slimscroll.min.js') }}"></script>
    <script src="{{ asset('plugins/fullcalendar/fullcalendar.js') }}"></script>
    <script src="{{ asset('plugins/fullcalendar/lang-all.js') }}"></script>
    <script src="{{ asset('plugins/fastclick/fastclick.js') }}"></script>
    <script src="{{ asset('dist/js/pages/dashboard.js') }}"></script>
{% endblock %}
