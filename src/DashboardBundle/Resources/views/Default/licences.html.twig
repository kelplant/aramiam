{% extends '::base.html.twig' %}
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/morris/morris.css') }}">
        <link rel="stylesheet" href="{{ asset('css/fullcalendar/fullcalendar.css') }}">
        <link rel="stylesheet" href="{{ asset('css/jvectormap/jquery-jvectormap-1.2.2.css') }}">
    {% endblock %}
    {% block title %} Accueil - Dashboard {% endblock %}
    {% block title2 %} Accueil {% endblock %}
    {% block title3 %} Licences {% endblock %}
    {% block breadcrumb %} Licences {% endblock %}
{% block body %}
    <div class="col-sm-12" id="dashboard-calendar">
        <section class="content">
            <div class="row">
                <table class="table table-striped display" id="bootstrap-todo">
                    <thead>
                    <tr>
                        <th class="admin_cat_table_title" style="width: 12%">Service</th>
                        <th colspan="3" class="admin_cat_table_title" style="width: 21%">Expert VO</th>
                        <th colspan="3" class="admin_cat_table_title" style="width: 21%">Conseiller Commercial</th>
                        <th colspan="3" class="admin_cat_table_title" style="width: 21%">Charg√© de Livraison</th>
                        <th colspan="3" class="admin_cat_table_title" style="width: 21%">Responsable d'Agence</th>
                    </tr>
                    </thead>
                    <tbody>
                    {% for key,value in odigoNumfinalTab %}
                    <tr id="generalBtr" data-href="">
                        <td class="admin_cat_table_cell" style="width: 12%; border-right: 1px solid #3e8ebd">{{ key }}</td>
                        <td class="admin_cat_table_cell" style="width: 7%">{{ value.Conseiller_Commercial.full }}</td>
                        <td class="admin_cat_table_cell" style="width: 7%">{{ value.Conseiller_Commercial.used }}</td>
                        <td class="admin_cat_table_cell {% if (value.Conseiller_Commercial.last <= 2) %} text-red {% endif %}{% if (value.Conseiller_Commercial.last > 2) %} text-green {% endif %}" style="width: 7%; border-right: 1px solid #3e8ebd">{{ value.Conseiller_Commercial.last }}</td>
                        <td class="admin_cat_table_cell" style="width: 7%">{{ value.Expert_VO.full }}</td>
                        <td class="admin_cat_table_cell" style="width: 7%">{{ value.Expert_VO.used }}</td>
                        <td class="admin_cat_table_cell {% if (value.Expert_VO.last <= 2) %} text-red {% endif %}{% if (value.Expert_VO.last > 2) %} text-green {% endif %}" style="width: 7%; border-right: 1px solid #3e8ebd">{{ value.Expert_VO.last }}</td>
                        <td class="admin_cat_table_cell" style="width: 7%">{{ value.Charge_de_Livraison.full }}</td>
                        <td class="admin_cat_table_cell" style="width: 7%">{{ value.Charge_de_Livraison.used }}</td>
                        <td class="admin_cat_table_cell {% if (value.Charge_de_Livraison.last <= 2) %} text-red {% endif %}{% if (value.Charge_de_Livraison.last > 2) %} text-green {% endif %}" style="width: 7%; border-right: 1px solid #3e8ebd">{{ value.Charge_de_Livraison.last }}</td>
                        <td class="admin_cat_table_cell" style="width: 7%">{{ value.Responsable_dAgence.full }}</td>
                        <td class="admin_cat_table_cell" style="width: 7%">{{ value.Responsable_dAgence.used }}</td>
                        <td class="admin_cat_table_cell {% if (value.Responsable_dAgence.last <= 2) %} text-red {% endif %}{% if (value.Responsable_dAgence.last > 2) %} text-green {% endif %}" style="width: 7%">{{ value.Responsable_dAgence.last }}</td>


                    </tr>
                    {% endfor %}
                    </tbody>
                </table>
            </div>
        </section>
    </div>
    <div class="col-sm-4" id="dashboard-right">

    </div>
    <div class="col-sm-12" id="dashboard-graphs">

    </div>
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('js/raphael/raphael-min.js') }}"></script>
    <script src="{{ asset('js/morris/morris.min.js') }}"></script>
    <script src="{{ asset('js/sparkline/jquery.sparkline.min.js') }}"></script>
    <script src="{{ asset('js/jvectormap/jquery-jvectormap-1.2.2.min.js') }}"></script>
    <script src="{{ asset('js/jvectormap/jquery-jvectormap-world-mill-en.js') }}"></script>
    <script src="{{ asset('js/moment/moment.min.js') }}"></script>
    <script src="{{ asset('js/slimScroll/jquery.slimscroll.min.js') }}"></script>
    <script src="{{ asset('js/fullcalendar/fullcalendar.js') }}"></script>
    <script src="{{ asset('js/fullcalendar/lang-all.js') }}"></script>
    <script src="{{ asset('js/fastclick/fastclick.js') }}"></script>
    <script src="{{ asset('js/dashboard/dashboard.js') }}"></script>
{% endblock %}
